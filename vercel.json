{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "SEED_API_SECRET": "@seed-api-secret"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/health/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/orders/**/*.ts": {
      "maxDuration": 60
    }
  },
  "crons": []
}

